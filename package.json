{
  "name": "adb",
  "version": "0.1.0",
  "description": "CMSC 191 UV Big Data Project",
  "main": "src/index.js",
  "scripts": {
    "test": "babel-tape-runner 'src/**/*.js' | faucet",
    "clean": "rimraf dist && mkdir -p dist",
    "build": "npm run clean && babel src -d dist -s --copy-files",
    "dev": "nodemon --watch 'src/**/*.js' --exec npm run start:dev",
    "start:dev": "node --require dotenv/config --require babel-register src/index.js",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMSC191-BigData-UV-Group1/aDB-server.git"
  },
  "keywords": [
    "SQL",
    "BigData",
    "CMSC191",
    "Project"
  ],
  "author": "CMSC 191 Big Data UV Group 1",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CMSC191-BigData-UV-Group1/aDB-server/issues"
  },
  "homepage": "https://github.com/CMSC191-BigData-UV-Group1/aDB-server#readme",
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-tape-runner": "2.0.1",
    "cross-env": "3.1.4",
    "faucet": "0.0.1",
    "nodemon": "1.11.0",
    "rimraf": "2.5.4",
    "tape": "4.6.3",
    "tape-async": "2.1.1"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "chalk": "1.1.3",
    "cors": "2.8.1",
    "debug": "2.6.1",
    "dotenv": "4.0.0",
    "express": "4.14.1",
    "lodash": "4.17.4"
  },
  "private": true
}
